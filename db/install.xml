<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/contractactivity/db" VERSION="20240621" COMMENT="XMLDB file for Moodle mod/contractactivity"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
<TABLES>
    <TABLE NAME="contractactivity_submissions" COMMENT="Respostas dos alunos">
        <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
            <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="formid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="ID da instância da atividade" />
            <FIELD NAME="fullname" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="nationality" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="civilstatus" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="profession" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="document" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="cpf" TYPE="text" NOTNULL="false"  SEQUENCE="false"/>
            <FIELD NAME="address" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="address_number" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="address_complement" TYPE="text" NOTNULL="false"  SEQUENCE="false"/>
            <FIELD NAME="address_neighbourhood" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="address_city" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="postal_code" TYPE="text" NOTNULL="true"  SEQUENCE="false"/>
            <FIELD NAME="done" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"  SEQUENCE="false"/>
            <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        </FIELDS>
        <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="users" REFFIELDS="id"/>
            <KEY NAME="courseid" TYPE="foreign" FIELDS="courseid" REFTABLE="courses" REFFIELDS="id"/>
        </KEYS>
    </TABLE>
    <TABLE NAME="contractactivity" COMMENT="Instâncias da atividade">
        <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
            <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
            <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
            <FIELD NAME="intro" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
            <FIELD NAME="completiononsubmission" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0"/>
            <FIELD NAME="introformat" TYPE="int" LENGTH="2" NOTNULL="false" SEQUENCE="false"/>
            <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
            <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        </FIELDS>
        <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            <KEY NAME="course" TYPE="foreign" FIELDS="course" REFTABLE="courses" REFFIELDS="id"/>
        </KEYS>
    </TABLE>
</TABLES>
</XMLDB>